@model List<IGrouping<string, EPManageWeb.Entities.Models.Clothes>>
@{
    ViewBag.Title = "Pinglei";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
@section head{}


<table class="table table-striped table-bordered">
    <tr>
        <td>品类</td>
        <td>使用次数</td>
    </tr>
    @foreach (var t in Model)
    {
        <tr>
            <td>@t.Key</td>
            <td>@t.Sum(t1 => t1.ViewCount)</td>
        </tr>
    }
</table>
