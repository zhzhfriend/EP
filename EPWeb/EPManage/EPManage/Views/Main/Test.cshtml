@{
    ViewBag.Title = "Test";
    Layout = null;
}
<html>
<head>

    <link href="@Styles.Url("~/Content/bootstrap/css/bootstrap.min.css")" rel="stylesheet"/>

    <script type="text/javascript" src="@Url.Content("~/Scripts/jquery-1.9.1.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/bootstrap.min.js")"></script>
</head>
<body>
    <!-- Button to trigger modal -->
    <div id="myCarousel" class="carousel slide">
        <!-- Carousel items -->
        <div class="carousel-inner">
            <div class="item"><img src="http://www.bootcss.com/assets/img/bootstrap-mdo-sfmoma-01.jpg" /></div>
            <div class="item"><img src="http://www.bootcss.com/assets/img/bootstrap-mdo-sfmoma-01.jpg" /></div>
            <div class="item"><img src="http://www.bootcss.com/assets/img/bootstrap-mdo-sfmoma-01.jpg" /></div>
        </div>
        <!-- Carousel nav -->
        <a class="carousel-control left" href="#myCarousel" data-slide="prev">&lsaquo;</a>
        <a class="carousel-control right" href="#myCarousel" data-slide="next">&rsaquo;</a>
    </div>

    <hr />

    <div id="myCarousel1" class="carousel slide">
        <ol class="carousel-indicators">
            <li data-target="#myCarousel1" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel1" data-slide-to="1"></li>
            <li data-target="#myCarousel1" data-slide-to="2"></li>
        </ol>
        <!-- Carousel items -->
        <div class="carousel-inner">
            <div class="active item"><img src="http://www.bootcss.com/assets/img/bootstrap-mdo-sfmoma-01.jpg" /></div>
            <div class="item"><img src="http://www.bootcss.com/assets/img/bootstrap-mdo-sfmoma-01.jpg" /></div>
            <div class="item"><img src="http://www.bootcss.com/assets/img/bootstrap-mdo-sfmoma-01.jpg" /></div>
        </div>
        <!-- Carousel nav -->
        <a class="carousel-control left" href="#myCarousel1" data-slide="prev">&lsaquo;</a>
        <a class="carousel-control right" href="#myCarousel1" data-slide="next">&rsaquo;</a>
    </div>
</body>
</html>
